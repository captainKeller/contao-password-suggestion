{
    "name": "presprog/contao-password-suggestion",
    "description": "Adds an one click password generator to the backend user management of Contao Open Source CMS to simplify adding new back end users.",
    "keywords": ["contao", "user", "password"],
    "type": "contao-module",
    "license": "LGPL3.0+",
    "authors": [
        {
            "name": "benedict zinke . present progressive",
            "email": "bz@presentprogressive.de",
            "homepage": "http://www.presentprogressive.de",
            "role": "Developer"
        }
    ],
    "require": {
        "contao/core-bundle": "~3.5 || ~4.3",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
    },
    "autoload": {
        "psr-4": {
            "PresProg\\PwSuggestion": "src/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "module": "system\\modules\\password-suggestion"
            }
        }
    }
}
